                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>noreply@textmaster.com</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="TMLibraryType">
                <display-name xml:lang="x-default">Site Library Type</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Private</display>
                        <value>private</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Shared</display>
                        <value>shared</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="TMMasterCatalogID">
