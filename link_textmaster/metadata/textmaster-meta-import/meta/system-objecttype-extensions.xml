    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="TMAPIEnvironment">
                <display-name xml:lang="x-default">API Environment</display-name>
                <description xml:lang="x-default">demo OR live (Set via API Setup page)</description>
